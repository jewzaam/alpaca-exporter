# Coverage configuration for alpaca-exporter
# Generated By: Cursor (Claude Sonnet 4.5)

[run]
source = src/
omit = 
    */utility.py
    */tests/*
    */__pycache__/*
    */site-packages/*

[report]
# Exclude common patterns from coverage
exclude_lines =
    # Standard exclusions
    pragma: no cover
    def __repr__
    if __name__ == .__main__.:
    
    # Exception handling
    raise AssertionError
    raise NotImplementedError
    
    # Debugging
    if DEBUG:
    
[html]
directory = htmlcov

[xml]
output = coverage.xml

